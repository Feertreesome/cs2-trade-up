{
  "name": "cs2-tradeup-ev",
  "type": "module",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm:server:dev\" \"npm:client:dev\"",
    "server:dev": "npm --workspace=cs2-tradeup-ev-server run dev",
    "client:dev": "npm --workspace=cs2-tradeup-ev-client run dev",
    "lint": "eslint \"{client,server}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{client,server}/**/*.{ts,tsx}\" --fix",
    "check": "npm run lint && tsc -b",
    "sync:floats": "tsx scripts/syncCollectionFloats.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@mui/styled-engine-sc": "^7.3.3",
    "bootstrap": "^5.3.8",
    "dotenv": "^17.2.2",
    "server": "^1.0.42"
  }
}
